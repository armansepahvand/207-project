extends layout

block content
  // Navigation
  nav.navbar.navbar-expand-md.navbar-light.bg-light.sticky-top
    .container
      a.navbar-brand(href="index.html")
        img(src="images/logogry.png" alt="logo" title="Sweetpea Travel's")
      h2 Sweetpea Travel's
      button.navbar-toggler(type="button" data-toggle="collapse" data-target="#navbarResponsive")
        span.navbar-toggler-icon
      #navbarResponsive.collapse.navbar-collapse
        ul.navbar-nav.ml-auto
          li.nav-item
            a.nav-link(href="/") Home
          li.nav-item
            a.nav-link(href="about") About
          li.nav-item
            a.nav-link.active(href="contact") Contact
          li.nav-item
            a.nav-link(href="register") Register
          if currentUser
            li.nav-item
              a.nav-link(href='logedinuser?id='+ currentUser._id) Profile
  .jumbotron.py-4
    div(style="text-align:center")
      h2 We would love to hear from you
  section#contact.contact
    .container.my-2
      .row
        .col-lg-6
          .info-box.mb-4
            i.bx.bx-map
            h3 Our Address
            p 1301 16 Ave NW, Calgary, AB T2M 0L4
        .col-lg-3.col-md-6
          .info-box.mb-4
            i.bx.bx-envelope
            h3 Email Us
            p info@sweetpeatravels.ca
        .col-lg-3.col-md-6
          .info-box.mb-4
            i.bx.bx-phone-call
            h3 Call Us
            p +1 587-913-2000
